{
    "manifest_version": 2,
    "name": "Rhyme & Bias Extension",
    "version": "1.0",
    "permissions": ["storage", "activeTab"],
    "browser_action": {
      "default_icon": "Project.jpg",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  